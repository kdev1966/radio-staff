# API Backend URL
# When running through nginx proxy, use /api
# When running locally, use http://localhost:4000/api
NEXT_PUBLIC_API_URL=/api

# Keycloak Authentication
# URL to Keycloak server (accessed from browser)
NEXT_PUBLIC_KEYCLOAK_URL=http://192.168.1.200:8080

# Keycloak realm name
NEXT_PUBLIC_KEYCLOAK_REALM=radio-staff

# Keycloak client ID for frontend
NEXT_PUBLIC_KEYCLOAK_CLIENT_ID=radio-frontend
